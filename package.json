{
  "name": "@jupiterone/graph-cbdefense",
  "version": "1.3.0",
  "description": "JupiterOne managed integration for Carbon Black Defense.",
  "license": "MPL-2.0",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "files": [
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "j1-integration collect",
    "graph": "j1-integration visualize",
    "graph:types": "j1-integration visualize-types",
    "lint": "eslint . --cache --fix --ext .ts,.tsx",
    "format": "prettier --write '**/*.{ts,js,json,css,md,yml}'",
    "type-check": "tsc",
    "test": "jest",
    "test:env": "LOAD_ENV=1 yarn test",
    "test:ci": "yarn lint && yarn type-check && yarn test",
    "build": "tsc -p tsconfig.dist.json --declaration && cp README.md dist/README.md",
    "prepush": "yarn lint && yarn type-check && jest --changedSince main"
  },
  "dependencies": {
    "@jupiterone/integration-sdk-core": "^6.18.0",
    "axios": "^0.21.1",
    "lodash": "^4.17.15",
    "type-fest": "^0.15.1"
  },
  "devDependencies": {
    "@jupiterone/integration-sdk-dev-tools": "^6.18.0",
    "@jupiterone/integration-sdk-testing": "^6.18.0",
    "@types/uuid": "^8.3.1",
    "uuid": "^8.3.2"
  }
}
